        console.log( row );
        $row = row;
        $('#FWAModalLabel').text("Edit Date for Site " + row.SiteID);
        $('#FWAexpDT').val(row.FWAexpDT);
        $('#piLicenseExp').val(row.piLicenseExp);
        $('#IRBexp').val(row.IRBexp);
        $('#FWAModal').modal();
    });

// FWA Save clicked
    $( "#FWASave" ).button().on( "click", function() {
	console.log("button clicked");

	var $FWAexpDT = $('#FWAexpDT').val(); 
	var $piLicenseExp = $('#piLicenseExp').val();
	var $IRBexp = $('#IRBexp').val();

	//console.log( $row.Comments );
	//console.log( $newComments.length );
        //console.log( "******** " + $revisitDate );

	    $.ajax({
		type: "POST",
		url: "http://baratheon.dipr.partners.org/api/updateFWA",
		data: "id=" + $row.id + "&FWAexpDT=" + $FWAexpDT + "&piLicenseExp=" + $piLicenseExp + "&IRBexp=" + $IRBexp,
		success: function() { 
		    console.log("success!!!"); 
		    $('#FWATable').bootstrapTable('refresh', {url: 'http://baratheon.dipr.partners.org/api/FWA/<%= $id %>'});
		},
	    });


         //createLog from recoverable PermStop table
	    $.ajax({
		type: "POST",
		url: "http://baratheon.dipr.partners.org/api/createLog",
		data: "siteid=<%= $id %>&" + "bwhStaff=#" + $username + "&message=#Admin : FWA exp. date : " + $FWAexpDT + " PI License exp. date : " + $piLicenseExp + " IRB exp. date : " + $IRBexp
    + "&hashtag=Admin " + $username,
		success: function() { 
		    console.log("success!!!"); 
		    updateTwitterSource("");
		},
	   });


	$('#FWAModal').modal('hide');
    });

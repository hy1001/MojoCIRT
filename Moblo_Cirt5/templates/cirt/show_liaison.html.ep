<script>

$(document).ready(function() {

    var $username = '<%== getUserName %>' ;

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeTAReport/"
		    }).then(function(data) {
			    $('#TA').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeLabIssues/"
		    }).then(function(data) {
			    $('#LabDataIssues').append("<small> (as of " + data.time + ") </small>");
			    $('#MissingLabs').append("<small> (as of " + data.time + ") </small>");
			    $('#EDCissues').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeTempStop/"
		    }).then(function(data) {
			    $('#TempStop').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeRecPermStop/"
		    }).then(function(data) {
			    $('#RecPermStop').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeMedRecs/<%= $raName %>"
		    }).then(function(data) {
			    $('#MedRecs').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeSiteMetrics/"
		    }).then(function(data) {
			    $('#SiteMetrics').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeV2/"
		    }).then(function(data) {
			    $('#V2').append("<small> (as of " + data.time + ") </small>");
			});

	$.ajax({
		url: "http://baratheon.dipr.partners.org/api/getUpdatedTimeV4/"
		    }).then(function(data) {
			    $('#V4').append("<small> (as of " + data.time + ") </small>");
			});

	$('.display').DataTable();
	
} ); <!-- doc ready -->


</script>



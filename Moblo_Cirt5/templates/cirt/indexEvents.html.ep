		   MyH['CONSDT'] =  $( '#CONSDT' ).val();
		   MyH['ReceivedDate'] =  $( '#ReceivedDate' ).val();
		   MyH['AGE'] =  $( '#AGE' ).val();
		   MyH['GENDER'] = $( '#GENDER' ).val();

		MyH['MI'] =  $( 'input[name="MI"]:checked' ).val();

